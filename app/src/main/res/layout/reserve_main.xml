<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/reserve_text_startsearch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="출발지 검색"
        android:inputType="text" />

    <EditText
        android:id="@+id/reserve_text_arrival"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="47dp"
        android:ems="10"
        android:hint="도착지 검색"
        android:inputType="text" />

    <Button
        android:id="@+id/reserve_btn_booking"
        android:layout_width="355dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/reserve_btn_callnow"
        android:layout_alignRight="@+id/reserve_btn_callnow"
        android:layout_alignParentTop="true"
        android:layout_marginTop="167dp"
        android:background="@drawable/wgborder"
        android:text="예약하기" />

    <Button
        android:id="@+id/reserve_btn_callnow"
        android:layout_width="356dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"

        android:layout_centerHorizontal="true"
        android:layout_marginTop="109dp"
        android:background="@drawable/rbborder"
        android:text="즉시콜"
        android:textColor="#FFFFFF" />

    <Button
        android:id="@+id/reserve_btn_medium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="110dp"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="224dp"
        android:background="@drawable/rbborder"
        android:text="중형" />

    <Button
        android:id="@+id/reserve_btn_mynum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="109dp"
        android:layout_marginLeft="109dp"
        android:layout_marginTop="351dp"
        android:background="@drawable/wgborder"
        android:text="내 번호" />

    <Button
        android:id="@+id/reserve_btn_oneway"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="111dp"
        android:layout_marginLeft="111dp"
        android:layout_marginTop="459dp"
        android:background="@drawable/wgborder"
        android:text="편도" />

    <Button
        android:id="@+id/reserve_btn_roundtrip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="240dp"
        android:layout_marginLeft="240dp"
        android:layout_marginTop="456dp"
        android:background="@drawable/rbborder"
        android:text="왕복" />

    <Button
        android:id="@+id/reserve_btn_anothernum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="241dp"
        android:layout_marginLeft="241dp"
        android:layout_marginTop="354dp"
        android:background="@drawable/rbborder"
        android:text="다른 번호" />

    <Button
        android:id="@+id/reserve_btn_large"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="242dp"
        android:layout_marginLeft="242dp"
        android:layout_marginTop="223dp"
        android:background="@drawable/wgborder"
        android:text="대형" />

    <TextView
        android:id="@+id/reserve_text_phonenum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/reserve_btn_anothernum"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="39dp"
        android:layout_marginLeft="39dp"
        android:layout_marginTop="18dp"
        android:text="연락처" />

    <TextView
        android:id="@+id/reserve_text_carsize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/reserve_btn_anothernum"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="39dp"
        android:layout_marginLeft="39dp"
        android:layout_marginTop="-113dp"
        android:text="차종" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/reserve_btn_large"
        android:layout_alignRight="@+id/reserve_btn_large"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="59dp"
        android:ems="10"
        android:hint="(다른 번호 입력)"
        android:inputType="phone" />


</RelativeLayout>